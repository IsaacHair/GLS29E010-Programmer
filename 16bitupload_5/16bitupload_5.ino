void setup() {
  int i, j;

  for (i = 0; i < 54; i++)
    pinMode(i, OUTPUT);

  digitalWrite(52, HIGH);
  digitalWrite(53, HIGH);
  
  for (i = 0; i < 160; i++, delay(50))
    if (i%2)
      digitalWrite(19, HIGH);
    else
      digitalWrite(19, LOW);

  for (i = 0; i < LEN; i++) {
    for (j = 0; j < 52; j++)
      digitalWrite(j, LOW);

    for (j = 0; j < 16; j++)
      if (prog[i][j])
        digitalWrite(j*2+21, HIGH);

    for (j = 35; j < 51; j++)
      if (prog[i][j])
        digitalWrite(j*2-50, HIGH);

    for (j = 16; j < 19; j++)
      if (prog[i][j])
        digitalWrite(j, HIGH);

    for (j = 19; j < 35; j++)
      if (prog[i][j])
        digitalWrite(j-19, HIGH);

    delay(10);
    digitalWrite(52, LOW);
    delay(10);
    digitalWrite(52, HIGH);
    delay(10);
  }

  digitalWrite(52, HIGH);
  digitalWrite(53, LOW);

  for (i = 0; i < 52; i++)
    if (i != 19)
      pinMode(i, INPUT);
}

void loop() {
  delayMicroseconds(2500);
  digitalWrite(19, LOW);
  delayMicroseconds(2500);
  digitalWrite(19, HIGH);
}
